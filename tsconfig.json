{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@cv": [
        "./cv.json"
      ],
      "@cv_english": [
        "./cv_english.json"
      ],
      "@/*": [
        "src/*"
      ]
    }
  }
}
